{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": false,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "sourceMap": true
    },
    "filesGlob": [
        "**/*.ts",
        "**/*.tsx"
    ],
    "exclude": [
        "node_modules",
        "bower_components",
        "data",
        "_site",
        "lib"
    ],
    "files": [
        "src/cql.ts",
        "src/constraint/encoding.ts",
        "src/constraint/spec.ts",
        "src/query/encoding.ts",
        "src/query/groupby.ts",
        "src/query/shorthand.ts",
        "src/query/spec.ts",
        "src/query/transform.ts",
        "src/ranking/effectiveness/effectiveness.ts",
        "src/ranking/effectiveness/channel.ts",
        "src/ranking/effectiveness/mark.ts",
        "src/ranking/aggregation.ts",
        "src/ranking/ranking.ts",
        "src/config.ts",
        "src/enumerator.ts",
        "src/enumspecindex.ts",
        "src/enumspec.ts",
        "src/generate.ts",
        "src/model.ts",
        "src/nest.ts",
        "src/property.ts",
        "src/schema.ts",
        "src/stylize.ts",
        "src/util.ts",
        "test/constraint/encoding.test.ts",
        "test/constraint/spec.test.ts",
        "test/query/encoding.test.ts",
        "test/query/groupby.test.ts",
        "test/query/query.test.ts",
        "test/query/shorthand.test.ts",
        "test/query/spec.test.ts",
        "test/ranking/effectiveness/channel.test.ts",
        "test/ranking/effectiveness/effectiveness.test.ts",
        "test/ranking/effectiveness/mark.test.ts",
        "test/ranking/aggregation.test.ts",
        "test/ranking/ranking.test.ts",
        "test/ranking/rule.ts",
        "test/enumerator.test.ts",
        "test/enumspec.test.ts",
        "test/enumspecindex.test.ts",
        "test/generate.test.ts",
        "test/model.test.ts",
        "test/nest.test.ts",
        "test/property.test.ts",
        "test/schema.test.ts",
        "test/stylize.test.ts",
        "typings/chai.d.ts",
        "typings/datalib.d.ts",
        "typings/mocha.d.ts"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
